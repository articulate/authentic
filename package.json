{
  "name": "@articulate/authentic",
  "version": "2.0.5",
  "description": "Proper validation of JWT's against JWK's",
  "main": "dist/index.js",
  "types": "dist/types.d.ts",
  "files": [
    "dist/*"
  ],
  "repository": "git@github.com:articulate/authentic.git",
  "author": "articulate",
  "license": "MIT",
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100
  },
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "tsc",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint .",
    "postversion": "git push --tags origin main",
    "preversion": "git checkout main",
    "test": "mocha",
    "test:ci": "yarn lint && yarn test:coverage && yarn coverage",
    "test:coverage": "nyc mocha --reporter=dot"
  },
  "dependencies": {
    "@hapi/boom": "^10.0.0",
    "jose": "^4.14.4"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "^8.47.0",
    "typescript": "^5.1.6"
  }
}
